{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "fmd-testfactory"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « password » de « FileServer1 »"
		},
		"FileServer2_password": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « password » de « FileServer2 »"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://FMD-Leak-Me.vault.azure.net/"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\WIN-H4RINI0KJ64\\FileShare_Documents"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Administrateur"
		},
		"FileServer2_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:/"
		},
		"FileServer2_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "none"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fmd-testfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"FileServer2_password": {
						"value": "[parameters('FileServer2_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"FileServer2_properties_typeProperties_host": {
						"value": "[parameters('FileServer2_properties_typeProperties_host')]"
					},
					"FileServer2_properties_typeProperties_userId": {
						"value": "[parameters('FileServer2_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}